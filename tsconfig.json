{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["dom", "es2015", "es2016", "es2017", "es2018", "es2019", "es2020"],
      "jsx": "react",
      "sourceMap": false,
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "declaration": true
